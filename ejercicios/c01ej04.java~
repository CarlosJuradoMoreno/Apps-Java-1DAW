public class c01ej04{
	public static void main(String[] args){
		System.out.println("Lunes\tMartes\tMiercoles\tJueves\tViernes");
		System.out.println("FOL\tPROG\tSIN\t\tPROG\tBBDD");
		System.out.println("FOL\tPROG\tSIN\t\tPROG\tBBDD");
		System.out.println("FOL\tPROG\tSIN\t\tPROG\tBBDD");
		System.out.println("PROG\tBBDD\tSIN\t\tED\tPROG");
		System.out.println("ED\tBBDD\tLM\tLM\t\tSIN");
		System.out.println("ED\tBBDD\tLM\tLM\t\tSIN");
	}
}
